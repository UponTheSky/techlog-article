{
  "name": "techlog",
  "version": "0.0.1",
  "description": "techlog: my personal blog with techical articles",
  "main": "./build/index.js",
  "scripts": {
    "lint": "eslint --ext .ts ./src",
    "format": "prettier --write ./src",
    "migrate:dev": "dotenv -e .env -- npx prisma migrate dev --name",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name init-test",
    "dev": "dotenv -e .env ts-node-dev ./src/server.ts",
    "test": "yarn migrate:test && dotenv -e .env.test jest --runInBand ./src/tests",
    "build": "tsc --project ./tsconfig.build.json",
    "start": "yarn build && node ./dist/server.js",
    "prepare": "husky install"
  },
  "repository": "https://github.com/UponTheSky/techlog.git",
  "author": "Sung Ju Yea <sailing.to.orbis@gmail.com>",
  "license": "None",
  "private": true,
  "engines": {
    "node": ">= 14.18.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.6.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  },
  "lint-staged": {
    "**/*.ts": "prettier --write --ignore-unknown "
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0"
  }
}
